@model IEnumerable<WebUI.Models.OrdersVM>

<h2>My Information:</h2>
<table class="table table-striped">
    <tr>
        <td>Name:</td>
        <td>@ViewBag.Name</td>
    </tr>
    <tr>
        <td>Address:</td>
        <td>@ViewBag.Address</td>
    </tr>
    <tr>
        <td>Email:</td>
        <td>@ViewBag.Contact</td>
    </tr>
    <tr>
        <td>UserName:</td>
        <td>@ViewBag.UserName</td>
    </tr>
    <tr>
        <td>Age:</td>
        <td>@ViewBag.Age</td>
    </tr>
    <tr>
        <td>Type of User:</td>
        <td>@ViewBag.Position</td>
    </tr>
    <tr>
        <td>Wallet:</td>
        <td>$@ViewBag.Currency</td>
    </tr>



</table>
<h2> My List of Orders</h2>

<table class="table table-striped table-hover">
    <tr>
        <th>Order Id Number</th>
        @* <th>Customer Id </th> *@

        <th>Store Id</th>
        <th>Product Id</th>
        <th>ProductName </th>
        <th>Amount </th>
        <th>Total Price of order</th>


    </tr>
    @* @foreach(StoreFrontVM item in Model)
        {
        <tr>
        <td>@Html.DisplayFor(modelItem => item.Id)</td>
        <td>@Html.DisplayFor(modelItem => item.Name)</td>
        <td>@Html.DisplayFor(modelItem => item.Address)</td>

        </tr>
        } *@
    @foreach (OrdersVM item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            @* <td>@Html.DisplayFor(modelItem => item.CustomerId)</td> *@
            <td>@Html.DisplayFor(modelItem => item.StoreFrontId)</td>
            <td>@Html.DisplayFor(modelItem => item.ItemId)</td>
            <td>@Html.DisplayFor(modelItem => item.ItemName)</td>
            <td>@Html.DisplayFor(modelItem => item.Quantity)</td>
            <td>$@Html.DisplayFor(modelItem => item.TotalPrice)</td>

        </tr>

    }
</table>
<a class="btn btn-primary text-white" asp-controller="Customer" asp-action="EditMyProfile">Edit Profile</a>